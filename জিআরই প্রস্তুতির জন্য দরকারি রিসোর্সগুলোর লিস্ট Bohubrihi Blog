@keyframes slideDownEnter { 
  0% { transform: translateY(-150%); }
  100% { transform: translateY(0px); }
}

@keyframes slideDownExit { 
  0% { transform: translateY(0px); }
  100% { transform: translateY(-150%); }
}

@keyframes slideUpEnter { 
  0% { transform: translateY(150%); }
  100% { transform: translateY(0px); }
}

@keyframes slideUpExit { 
  0% { transform: translateY(0px); }
  100% { transform: translateY(150%); }
}

