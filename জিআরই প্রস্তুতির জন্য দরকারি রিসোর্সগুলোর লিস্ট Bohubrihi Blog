#onesignal-popover-container #onesignal-popover-dialog .popover-button.primary { box-shadow: rgba(0, 0, 0, 0.16) 0px 2px 5px 0px, rgba(0, 0, 0, 0.12) 0px 2px 6px 0px; background: rgb(17, 101, 241) !important; color: rgb(255, 255, 255) !important; }

